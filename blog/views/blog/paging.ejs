<div class="row-fluid">
    <div class="span3 offset9">
        <ul class="pager">
            <% if(!isFirstPage) {%>
            <li>
                <a class="pull-right" title="上一页" href="?p=<%= page-1 %>">上一页</a>
            </li>
            <% }%>
            <% if(!isLastPage){ %>
            <li>
                <a class="pull-right" title="下一页" href="?p=<%= page+1 %>">下一页</a>
            </li>
            <% } %>
        </ul>
    </div>
</div>